\mode<presentation>

% Theme colors
\definecolor{OceanLightGray}{RGB}{235, 237, 236}   % #ebedec
\definecolor{OceanGray}{RGB}{201,213,216}          % #c9d5d8
\definecolor{OceanBrown}{RGB}{115,115,115}         % #737373
\definecolor{OceanBlue}{RGB}{149,184,204}          % #95b8cc
\definecolor{lucidcolor1}{RGB}{203,221,233}        % #CBDDE9
\definecolor{lucidcolor2}{RGB}{149,184,204}        % #95b8cc 
\definecolor{lucidcolor3}{RGB}{173,213,213}        % #add5d5
\definecolor{lucidcolor4}{RGB}{89,142,146}         % #598e92

\definecolor{lucidcolor5}{RGB}{233,175,175}        % #e9afaf 

\definecolor{lucidfont1}{RGB}{0,0,0}           % #ff5555

% Background Color
\setbeamercolor*{background canvas}{bg=white}

% Normal text
\setbeamercolor{normal text}{fg=lucidfont1!75}

% First page
\setbeamercolor*{title}{fg=lucidfont1!75}
\setbeamercolor*{subtitle}{fg=lucidfont1!75}
\setbeamercolor*{author}{fg=lucidfont1!75}
\setbeamercolor*{date}{fg=lucidfont1!75}
	
% Generic frames header and footer
\setbeamercolor{titlelike}{fg=black}
\setbeamercolor*{frametitle}{fg=black}
\setbeamercolor*{framesubtitle}{fg=black}
\setbeamercolor*{title in head/foot}{fg=black}
\setbeamercolor*{author in head/foot}{fg=black}

%Blocks
\setbeamercolor{block title}{bg=lucidcolor2,fg=black}
\setbeamercolor{block body}{bg=OceanLightGray,fg=black}

\setbeamercolor{block title alerted}{bg=lucidcolor5,fg=black}
\setbeamercolor{block body alerted}{bg=lucidcolor5!50,fg=black}

\setbeamercolor{block title example}{bg=lucidcolor4,fg=black}
\setbeamercolor{block body example}{bg=lucidcolor4!20,fg=black}

% Lists
\setbeamercolor*{item}{{fg=lucidcolor3}}
%\setbeamercolor*{itemize item}{fg=light-grey}

% Quotation and quote
\setbeamercolor*{quotation}{fg=lucidcolor1,bg=OceanBlue!5}

% Modificacion del color para el header del frame
\setbeamercolor*{frametitle-left}{fg=lucidcolor3,bg=red!5}
\setbeamercolor*{frametitle-bottom}{fg=lucidcolor2,bg=red!5}


%Normal
\setbeamercolor*{normal text}{fg=lucidfont1!75}

